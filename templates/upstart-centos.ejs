respawn

limit nofile 1000000 1000000

script<% if (uid) { %>
  su - <%- uid %><% } %>
  cd <%- workingDirectory %>
  exec <% Object.keys(env).forEach(function(key) {%><%- key %>=<%- env[key] %> <% }); %><%- nodeBin %><% nodeFlags.forEach(function(flag) { %> <%- flag %><% }); %> <%- startScript %><% flatArgs.forEach(function(arg) { %> <%- arg %><% }); %> >> <%- logFile %> 2>&1
end script
