# <%= description %>

stop on runlevel [S016]

respawn

limit nofile 1000000 1000000

script
  cd <%= workingDirectory %>
  <% Object.keys(env).forEach(function(key) {%><%= key %>=<%= env[key] %> <% }); %><%= nodeBin %> <%= bin %><% args.forEach(function(arg, n) { %> <%= arg %><% }); %> >> <%= logFile %> 2>&1
end script
